{
  "name": "vstsexttask",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "package": "npm run clean  && npm run compile  && tfx extension create --rev-version",
    "publish": "tfx extension publish --no-wait-validation",
    "clean": "rimraf ./*.vsix",
    "compile": "tsc -p .",
    "install-lib": "cd remaneVariables && npm install",
    "test": "npm run compile && mocha remaneVariables/tests/_suite.js",
    "update": "ncu -u"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.17",
    "sync-request": "^6.1.0",
    "tfx-cli": "^0.7.11",
    "typescript": "3.7.3"
  }
}
